@model List<BIBSI.Models.Ilan>

@{
    ViewBag.Title = "ListIlan";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<h2>Tüm İlanlar</h2>

<div id="tumIlanlar">
    @{
        foreach (var ilan in Model)
        {
            <div class="ilan">
                <h2>@ilan.IsTanimi</h2>
                <p>@ilan.Aciklama</p>
            </div>
        }
    }
</div>


@section scripts{
    <script>
        menuSec(3);
    </script>
}